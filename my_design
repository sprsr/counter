    .scope S_0x564bbcaba670;
T_0 ;
    %wait E_0x564bbca816e0;
    %load/vec4 v0x564bbcaccd60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x564bbcaccc80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x564bbcaccc80_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x564bbcaccc80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564bbcaba4e0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564bbcaccf80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564bbcacceb0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x564bbcaba4e0;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564bbcaccf80_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564bbcaccf80_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564bbcaccf80_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564bbcaccf80_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x564bbcaba4e0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x564bbcacceb0_0;
    %nor/r;
    %store/vec4 v0x564bbcacceb0_0, 0, 1;
    %jmp T_3;
